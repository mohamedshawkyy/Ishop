<main role="main" class="container">
   <br>

     <% if (message) { %>
      <div class="alert alert-<%= message.type %>">
       <button type="button" class="close" data-dismiss="alert" >x</button>
      <strong><%= message.intro %> </strong> <%= message.message %>  
      </div> 
      <% } %> 
    
</main>
    